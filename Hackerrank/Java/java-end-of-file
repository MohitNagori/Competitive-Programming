import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
    private static Scanner sc = new Scanner(System.in);

    public static void main(String[] args) {
        for (int i = 1; sc.hasNext(); i++) {
            System.out.println(i + " " + sc.nextLine());
        }
    }
}
